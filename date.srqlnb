[{"kind":2,"language":"surrealql","value":"BEGIN TRANSACTION;\n\nlet $input = \"2024-03-01T22:01:00+02:00\";\n\nRETURN {\n    year : time::year($input),\n    day : time::day($input),\n    month : time::month($input),\n    hour : time::hour($input),\n    minute : time::minute($input),\n    second : time::second($input),\n    unix : time::unix($input)\n};\n\nCOMMIT TRANSACTION;"},{"kind":2,"language":"surrealql","value":"RETURN {\n    now: time::now(),\n    timezones : time::timezone()\n}"},{"kind":2,"language":"surrealql","value":"RETURN [\n    time::from::unix(0),\n    time::from::secs(1),\n    time::from::millis(0),\n    time::from::micros(0),\n    time::from::nanos(0)\n]"},{"kind":2,"language":"surrealql","value":"RETURN time::format(time::now(), '%d/%m/%Y')"}]