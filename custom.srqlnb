[{"kind":2,"language":"surrealql","value":"DEFINE FUNCTION fn::cleanString($input: string) {\n    LET $array = string::words($input);\n    LET $clean = array::join($array,\" \");\n    RETURN $clean;\n};"},{"kind":2,"language":"surrealql","value":"RETURN fn::cleanString(\" hello    world     \");"},{"kind":2,"language":"surrealql","value":"DEFINE FUNCTION fn::isPalyndrome($input: string) {\n    LET $cleaned = fn::cleanString(string::lowercase($input));\n    LET $reversed = string::reverse($cleaned);\n    RETURN $cleaned == $reversed;\n};"},{"kind":2,"language":"surrealql","value":"RETURN fn::isPalyndrome(\"Malayalam \");\nRETURN fn::isPalyndrome(\"notpalyndrome\");"},{"kind":2,"language":"surrealql","value":"DEFINE FUNCTION fn::applyTaxes($price: number, $taxes: number) {\n    LET $taxed = $price + $price * $taxes;\n    RETURN $taxed;\n};"},{"kind":2,"language":"surrealql","value":"RETURN fn::applyTaxes(112,0.30)"}]