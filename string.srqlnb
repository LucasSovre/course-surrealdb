[{"kind":2,"language":"surrealql","value":"RETURN \"hello world\";"},{"kind":2,"language":"surrealql","value":"RETURN [\n    \"hello \" + \"world\",\n    string::concat(\"hello \",\"world\")\n];"},{"kind":2,"language":"surrealql","value":"RETURN string::split(\"cat,bird,dog\",\",\");"},{"kind":2,"language":"surrealql","value":"RETURN string::join(',',\"cat\",\"bird\",\"dog\");"},{"kind":2,"language":"surrealql","value":"RETURN [\n    string::words(\"la vie en rose\"),\n    string::split(\"la vie en rose\",\" \")\n]"},{"kind":2,"language":"surrealql","value":"RETURN [\n    string::words(\"  la   vie   en rose\"),\n    string::split(\"  la   vie   en rose\",\" \")\n]"},{"kind":2,"language":"surrealql","value":"RETURN string::replace(\"la vie en rose\",\"rose\",\"jaune\");"},{"kind":1,"language":"markdown","value":"# FORMAT"},{"kind":2,"language":"surrealql","value":"BEGIN TRANSACTION;\n\nlet $string = \"hello EVERyBOdy\";\n\nRETURN [\n    string::lowercase($string),\n    string::uppercase($string),\n    string::slug($string)\n];\n\nCOMMIT TRANSACTION;"},{"kind":2,"language":"surrealql","value":"RETURN [\n    string::contains(\"hello world\",\"world\"),\n    string::startsWith(\"hello world\", \"hello\"),\n    string::endsWith(\"hello world\",\"world\"),\n    string::is::alphanum(\"helloworld\"),\n    string::is::datetime(\"01/01/2021 01:00:00\",\"%d/%m/%Y %H:%M:%S\"),\n    string::is::email(\"user@gmail.com\"),\n    string::is::url(\"http://google.fr\"),\n    string::is::domain('google.fr')\n];"},{"kind":2,"language":"surrealql","value":"BEGIN TRANSACTION;\n\nlet $email = \"user@gmail.com\";\nlet $url = \"http://google.com:443/path?search=hello#world\";\n\nRETURN [\n    parse::email::host($email),\n    parse::email::user($email),\n    parse::url::domain($url),\n    parse::url::fragment($url),\n    parse::url::host($url),\n    parse::url::path($url),\n    parse::url::port($url),\n    parse::url::query($url)\n];\n\nCOMMIT TRANSACTION;"}]